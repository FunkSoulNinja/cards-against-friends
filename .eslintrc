{
    "extends": "rallycoding",
    "rules": {
        "react/require-extension": "off",
        "linebreak-style": 0,
        "eol-last": 1,
        "arrow-body-style": 0,
        "quotes": 0,
        "no-multiple-empty-lines": 0,
        "max-len": 0,
        "import/no-named-as-default-member": 0,
        "react/sort-comp": 0,
        "prefer-const": 0,
        "object-shorthand": 0,
        "prefer-arrow-callback": 0,
        "prefer-template": 0,
        "padded-blocks": 0,
        "import/newline-after-import": 0,
        "global-require": 0
        },
    "env": {
        "browser": true,
        "jasmine": true
        },
    "parser": "babel-eslint",
    "globals": {
        "process": false,
        "IS_PRODUCTION": false,
        "IS_DEVELOPMENT": false
        }
}
